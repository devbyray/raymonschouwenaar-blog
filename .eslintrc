{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error", "info"]
      }
    ]
  },
  "plugins": [ "import" ],
  "globals": {
    "__dirname": true,
    "require": true,
    "process": true,
    "ENV": true,
    "module": true
  }
}